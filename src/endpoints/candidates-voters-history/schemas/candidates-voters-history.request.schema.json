{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "candidates-voters-history.request.schema.json",
    "title": "/candidates_voters_history?dacId=${dacId}&candidateId={candidateId}&skip={skip}&limit={limit} request schema",
    "type": "object",
    "properties": {
        "query": {
            "type": "object",
            "properties": {
                "dacId": {
                    "$ref": "#/$defs/dacId"
                },
                "candidateId": {
                    "$ref": "#/$defs/account"
                },
                "skip": {
                    "$ref": "#/$defs/zeroOrGreaterInteger"
                },
                "limit": {
                    "$ref": "#/$defs/positiveInteger"
                }
            },
            "required": [
                "dacId",
                "candidateId"
            ]
        }
    },
    "$defs": {
        "dacId": {
            "type": "string",
            "minLength": 5,
            "maxLength": 6
        },
        "account": {
            "type": "string",
            "minLength": 1
        },
        "zeroOrGreaterInteger": {
            "type": "integer",
            "minimum": 0
        },
        "positiveInteger": {
            "type": "integer",
            "minimum": 1
        }
    }
}